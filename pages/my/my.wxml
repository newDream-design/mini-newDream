<view class='main'>
    <view class='main-mid'>
        <view class='myBanner whiteBanner'>
            <view class='userInfo'>
                <open-data class='userAvatar' type="userAvatarUrl"></open-data>
                <open-data class='userName' type="userNickName"></open-data>
            </view>
            <!-- 需要使用 button 来授权登录 -->
            <button wx:if="{{canIUse}}" open-type="getUserInfo" bindgetuserinfo="bindGetUserInfo">授权登录</button>
        </view>
        <view class='nav-wrap'>
            <navigator url='/pages/allOrders/allOrders?currentTab=0' class='weui-cell weui-cell_access allOrder' hover-class='weui-cell_active'>
                <view class='weui-cell__bd'>全部订单</view>
                <view class='weui-cell__ft weui-cell__ft_in-access'></view>
            </navigator>
        </view>
        <view class='order-type'>
            <view>
                <navigator url='order/allOrders?currentTab=1' class='order-nav' hover-class='none'>
                    <text class='iconfont icon-daizhifu'></text>
                    <!-- <image src='/images/state1.png'></image> -->
                    <text>待支付</text>
                    <text class='count' wx:if="{{count.toPay>0}}">{{count.toPay}}</text>
                </navigator>
            </view>
            <view>
                <navigator url='order/allOrders?currentTab=2' class='order-nav' hover-class='none'>
                    <!-- <image src='/images/state2.png'></image> -->
                    <text class='iconfont icon-daifahuo'></text>
                    <text>待发货</text>
                    <text class='count' wx:if="{{count.toDeliver}}">{{count.toDeliver}}</text>
                </navigator>
            </view>
            <view>
                <navigator url='order/allOrders?currentTab=3' class='order-nav' hover-class='none'>
                    <!-- <image src='/images/state3.png'></image> -->
                    <text class='iconfont icon-daishouhuo'></text>
                    <text>待收货</text>
                    <text class='count' wx:if="{{count.toReceive>0}}">{{count.toReceive}}</text>
                </navigator>
            </view>
            <view>
                <navigator url='order/allOrders?currentTab=4' class='order-nav' hover-class='none'>
                    <!-- <image src='/images/state4.png'></image> -->
                    <text class='iconfont icon-weibiaoti2fuzhi06'></text>
                    <text>待评价</text>
                    <text class='count' wx:if="{{count.toComment>0}}">{{count.toComment}}</text>
                </navigator>
            </view>
            <view>
                <navigator url='order/allOrders?currentTab=5' class='order-nav' hover-class='none'>
                    <!-- <image src='/images/state5.png'></image> -->
                    <text class='iconfont icon-tuikuan'></text>
                    <text>退款</text>
                    <text class='count' wx:if="{{count.toRefund>0}}">{{count.toRefund}}</text>
                </navigator>
            </view>
        </view>

        <view class='nav-wrap nav-wrap2'>
            <view class='weui-cell weui-cell_access' hover-class='weui-cell_active' bindtap='address'>
                <view class='weui-cell__bd'>收货地址</view>
                <view class='weui-cell__ft weui-cell__ft_in-access'></view>
            </view>
        </view>

        <view class='nav-wrap nav-wrap2'>
            <navigator url='/pages/collection/collection' class='weui-cell weui-cell_access' hover-class='weui-cell_active'>
                <view class='weui-cell__bd'>我的收藏</view>
                <view class='weui-cell__ft weui-cell__ft_in-access'></view>
            </navigator>
        </view>

        <view class='nav-wrap nav-wrap2'>
            <view class='weui-cell weui-cell_access' hover-class='weui-cell_active'>
                <view class='weui-cell__bd'>
                    <button open-type='contact' class='contact'>联系客服</button>
                </view>
                <view class='weui-cell__ft weui-cell__ft_in-access'></view>
            </view>
        </view>

        <view class='nav-wrap nav-wrap2'>
            <navigator url='/pages/aboutUs/aboutUs' class='weui-cell weui-cell_access' hover-class='weui-cell_active'>
                <view class='weui-cell__bd'>关于我们</view>
                <view class='weui-cell__ft weui-cell__ft_in-access'></view>
            </navigator>
        </view>

    </view>
    <view class='main-bot'>
        <import src='/pages/template/support.wxml' />
        <template is='support' />
    </view>
</view>