Page {
    background-color: #fff;
}

.myBanner {
    position: relative;
    min-height: 5rem;
    overflow: hidden;
}

.whiteBanner {
    background-color: var(--colorBlue);
}

.userInfo {
    position: absolute;
    display: flex;
    align-items: center;
    background-color: var(--colorBlue);
    padding: 1rem;
}

.userInfo::after {
    border: none;
}

.userAvatar {
    width: 3rem;
    height: 3rem;
    border-radius: 50%;
    display: flex;
    overflow: hidden;
}

.userName {
    color: #fff;
    font-size: 32rpx;
    margin-left: 1rem;
    font-weight: bold;
}

.myBanner .my {
    position: absolute;
    left: 30rpx;
    top: 0;
    height: 300rpx;
    display: flex;
    align-items: center;
}

.avatar {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}

.nickName {
    font-size: 32rpx;
    color: #fff;
    margin-left: 20rpx;
}

.allOrder {
    border-top: none 0;
}

.nav-wrap {
    background-color: #fff;
}

.weui-cell__bd {
    font-size: 32rpx;
    color: #333;
}

.order-type {
    height: 174rpx;
    background-color: #fff;
    display: flex;
    align-items: center;
    justify-content: space-around;
}

.order-nav .iconfont {
    /* width: 50rpx;
  height: 50rpx; */
    display: block;
    margin: 0 auto;
    font-size: 50rpx;
}

.order-nav text {
    display: block;
    text-align: center;
    color: #555;
    font-size: 28rpx;
}

.nav-wrap2 {
    margin-top: 20rpx;
}

.order-type view {
    position: relative;
}

.count {
    position: absolute;
    top: -0.4em;
    right: 20rpx;
    display: inline-block;
    height: 1.6em;
    min-width: 1.6em;
    line-height: 1.6;
    padding: 0 0.4em;
    font-size: 20px;
    border-radius: 0.8em;
    background-color: var(--bgcolor);
    color: #fff !important;
    text-align: center;
    white-space: nowrap;
    transform: translateX(50%) scale(0.6);
    transform-origin: center;
    z-index: 10;
    box-shadow: 0 0 0 2px #fff;
    box-sizing: border-box;
}

.contact {
    padding: 0;
    margin: 0;
    background: none !important;
    font-size: 32rpx;
    color: #333;
    text-align: left;
    line-height: normal;
}

.contact::after {
    border: none 0;
}

.login-btn {
    padding: 0;
    margin: 0;
    border-radius: 0;
    background-color: transparent;
}

.login-btn::after {
    border: 0;
}

/* 自定义 */

.ticket {
	width: 19rem;
    margin: 0.5rem;
	height: 3.5rem;
    background: linear-gradient(to right, #f8f3eb, #d9c39a);
    display: flex;
    align-items: center;
    border-radius: 5px;
    justify-content: space-between;
}

.ticket view {
    display: flex;
    align-items: center;
}

.ticket view .icon {
    width: 3rem;
    height: 1.5rem;
    margin: 0.5rem;
}

.ticket view .arrow-right:after {
    border-color: #000;
    margin-right: 15px;
}

.server-warp {
    width: 100%;
    display: flex;
    flex-wrap: wrap;
}

.server-warp navigator, .server-warp view button {
    width: 5rem;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-bottom: 0.5rem;
}

.server-warp image {
    width: 3rem;
    height: 3rem;
    margin-bottom: 0.25rem;
}

.server-warp text {
    font-size: 14px;
    color: #666;
    line-height: 1;
}

.AddressButtonCover {
    padding-left: 0;
    padding-right: 0;
    line-height: 1;
    border-radius: 0;
    background-color: #fff;
    width: 5rem;
    height: 5rem;
}

.AddressButtonCover::after {
    border: none;
}
