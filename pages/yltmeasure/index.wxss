.measureData-wrap {
    display: flex;
    flex-direction: column;
    margin: 0.25rem 0 0.25rem 0;
    background-color: #fff;
}

.measureData-wrap .left {
    width: 4rem;
}

.measureData-wrap .right {
    width: 16rem;
}

.measureData-wrap .title-wrap {
    display: flex;
    flex-direction: row;
    margin: 1rem;
    justify-content: space-between;
}

.measureData-wrap .title {
    width: auto;
    max-width: 16rem;
    color: #333;
    font-size: 16px;
    font-weight: 700;
}

.measureData-wrap .dataTag {
    width: 60px;
    text-align: center;
    font-size: 14px;
    color: #aaa;
    padding: 2px 10px 2px 10px;
    border-radius: 10px;
    border: 1px solid #aaa;
}

.measureData-wrap .dataTag.default {
    width: 60px;
    text-align: center;
    font-size: 14px;
    color: #aaa;
    padding: 2px 10px 2px 10px;
    border-radius: 10px;
    border: 1px solid #aaa;
}

.measureData-wrap .data-wrap {
    display: flex;
    flex-wrap: wrap;
}

.measureData-wrap .data-wrap .data {
    width: 5.5rem;
    color: #888;
    font-size: 13px;
    margin: 0.25rem 0 0.25rem 1rem;
}
