.measureData-wrap {
    display: flex;
    flex-direction: column;
    margin: 0.25rem 0 0.75rem 0;
    padding-bottom: 0.5rem;
    background-color: #fff;
}

.measureData-wrap .title-wrap {
    display: flex;
    flex-direction: row;
    margin: 1rem;
    justify-content: space-between;
}

.measureData-wrap .title-wrap .left {
    width: 16rem;
    display: flex;
    flex-direction: row;
}

.measureData-wrap .title-wrap .right {
    width: 3rem;
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
}

.measureData-wrap .title-wrap .left .title {
    width: auto;
    max-width: 16rem;
    color: #333;
    font-size: 16px;
    font-weight: 700;
}

.measureData-wrap .title-wrap .left .dataTag {
    width: 60px;
    text-align: center;
    font-size: 14px;
    color: #aaa;
    padding: 2px 10px 2px 10px;
    margin: 0 15px 0 15px;
    border-radius: 10px;
    border: 1px solid #aaa;
}

.measureData-wrap .title-wrap .left .dataTag.default {
    color: #fff;
    background: var(--colorGold);
    border: 1px solid var(--colorGold);
}

.measureData-wrap .data-wrap {
    display: flex;
    flex-wrap: wrap;
    padding-bottom: 0.5rem;
}

.measureData-wrap .data-wrap .data {
    width: 5.5rem;
    height: 25px;
    color: #888;
    font-size: 13px;
    margin: 0.25rem 0 0.25rem 1rem;
}

.addMeasureData {
    width: 8rem;
    height: 3rem;
    margin:1rem auto;
    background: var(--colorBlue);
    display: flex;
    justify-content: center;
    align-items: center;
}

.addMeasureData text{
    font-size: 16px;
    text-align: center;
    color:#FFF;
}