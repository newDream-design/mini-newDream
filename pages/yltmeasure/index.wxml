<view class="measureData-wrap" wx:for="{{measure.measureData}}" wx:for-item="data" wx:key="md">
    <!--<view class="left"></view>-->
    <!--<view class="right"></view>-->
    <view class="title-wrap">
        <view class="title">
            <text>{{data.measureName}}</text>
        </view>
        <view class="dataTag default">
            <text>设为默认</text>
        </view>
    </view>
    <view class="data-wrap">
        <view class="data" wx:for="{{data.details}}" wx:for-index="key" wx:for-item="value" wx:key="ds">{{key}}：{{value}}</view>
    </view>
</view>